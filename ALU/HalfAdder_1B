v0x7f802f01b2f0_0 .var "i", 2 0;
v0x7f802f01b3b0_0 .var "input_a", 0 0;
v0x7f802f01b470_0 .var "input_b", 0 0;
v0x7f802f01b540_0 .net "output_carry", 0 0, L_0x7f802f01b6c0;  1 drivers
v0x7f802f01b5f0_0 .net "output_sum", 0 0, L_0x7f802f01b810;  1 drivers
S_0x7f802f00b0a0 .scope module, "h" "HalfAdder_1B" 3 5, 4 1 0, S_0x7f802f004a90;
 .timescale 0 0;
    .port_info 0 /INPUT 1 "a";
    .port_info 1 /INPUT 1 "b";
    .port_info 2 /OUTPUT 1 "carry";
    .port_info 3 /OUTPUT 1 "sum";
L_0x7f802f01b6c0 .functor AND 1, v0x7f802f01b3b0_0, v0x7f802f01b470_0, C4<1>, C4<1>;
L_0x7f802f01b810 .functor XOR 1, v0x7f802f01b3b0_0, v0x7f802f01b470_0, C4<0>, C4<0>;
v0x7f802f0059f0_0 .net "a", 0 0, v0x7f802f01b3b0_0;  1 drivers
v0x7f802f01adb0_0 .net "b", 0 0, v0x7f802f01b470_0;  1 drivers
v0x7f802f01ae50_0 .net "carry", 0 0, L_0x7f802f01b6c0;  alias, 1 drivers
v0x7f802f01af00_0 .net "sum", 0 0, L_0x7f802f01b810;  alias, 1 drivers
S_0x7f802f01b000 .scope task, "test" "test" 3 7, 3 7 0, S_0x7f802f004a90;
 .timescale 0 0;
v0x7f802f01b1d0_0 .var "A", 0 0;
v0x7f802f01b260_0 .var "B", 0 0;
TD_HalfAdder_1B_t.test ;
    %load/vec4 v0x7f802f01b1d0_0;
    %store/vec4 v0x7f802f01b3b0_0, 0, 1;
    %load/vec4 v0x7f802f01b260_0;
    %store/vec4 v0x7f802f01b470_0, 0, 1;
    %delay 100, 0;
    %end;
    .scope S_0x7f802f004a90;
T_1 ;
    %vpi_call/w 3 14 "$dumpfile", "result.vcd" {0 0 0};
    %vpi_call/w 3 15 "$dumpvars" {0 0 0};
    %pushi/vec4 0, 0, 3;
    %store/vec4 v0x7f802f01b2f0_0, 0, 3;
T_1.0 ;
    %load/vec4 v0x7f802f01b2f0_0;
    %cmpi/u 4, 0, 3;
    %jmp/0xz T_1.1, 5;
    %load/vec4 v0x7f802f01b2f0_0;
    %parti/s 1, 0, 2;
    %store/vec4 v0x7f802f01b1d0_0, 0, 1;
    %load/vec4 v0x7f802f01b2f0_0;
    %parti/s 1, 1, 2;
    %store/vec4 v0x7f802f01b260_0, 0, 1;
    %fork TD_HalfAdder_1B_t.test, S_0x7f802f01b000;
    %join;
    %load/vec4 v0x7f802f01b2f0_0;
    %addi 1, 0, 3;
    %store/vec4 v0x7f802f01b2f0_0, 0, 3;
    %jmp T_1.0;
T_1.1 ;
    %vpi_call/w 3 20 "$finish" {0 0 0};
    %end;
    .thread T_1;
